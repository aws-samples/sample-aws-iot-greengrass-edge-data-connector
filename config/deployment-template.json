{
  "targetArn": "arn:${AWS_PARTITION}:iot:${AWS_REGION}:${AWS_ACCOUNT_ID}:thing/${GREENGRASS_CORE_DEVICE}",
  "components": {
    "aws.greengrass.StreamManager": {
      "componentVersion": "${STREAM_MANAGER_VERSION}"
    },
    "com.example.DebeziumEmbeddedComponent": {
      "componentVersion": "${DEBEZIUM_COMPONENT_VERSION}"
    },
    "com.example.SFTPToS3Component": {
      "componentVersion": "${SFTP_COMPONENT_VERSION}"
    },
    "com.example.MySQLToS3Component": {
      "componentVersion": "${MYSQL_COMPONENT_VERSION}"
    }
  },
  "deploymentPolicies": {
    "failureHandlingPolicy": "ROLLBACK",
    "componentUpdatePolicy": {
      "timeoutInSeconds": 300,
      "action": "NOTIFY_COMPONENTS"
    },
    "configurationValidationPolicy": {
      "timeoutInSeconds": 60
    }
  }
}
